{
  "name": "next-starter-kit",
  "version": "1.0.0",
  "description": "ssr by next.js",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development node server/index.js",
    "build": "NODE_ENV=production next build ./src",
    "lint": "eslint './src/**/*.js'",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "linters": {
      "./src/**/*.js": [
        "eslint --fix",
        "git add"
      ]
    }
  },
  "keywords": [
    "next.js",
    "koa2",
    "node",
    "ssr",
    "react",
    "redux",
    "webpack"
  ],
  "author": "memotail",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.6",
    "dotenv": "^6.0.0",
    "dotenv-webpack": "^1.5.7",
    "husky": "^0.14.3",
    "isomorphic-fetch": "^2.2.1",
    "koa": "^2.5.3",
    "koa-compress": "^3.0.0",
    "koa-router": "^7.4.0",
    "lint-staged": "^7.3.0",
    "next": "^7.0.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "eslint": "^5.6.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "webpack-bundle-analyzer": "^3.0.2"
  }
}
